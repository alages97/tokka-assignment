{
  "swagger": "2.0",
  "info": {
      "title": "Uniswap API",
      "description": "APIs regarding Uniswap transactions",
      "version": "1.0"
  },
  "basePath": "/api",
  "schemes": ["http"],
  "consumes": ["application/json"],
  "produces": ["application/json"],
  "paths": {
      "/getFee": {
          "get": {
              "summary": "Returns fee in USDT for a Uniswap WETH-USDC transaction",
              "description": "Returns the fee in USDT for a Uniswap transaction between WETH and USDC tokens.",
              "parameters": [
                  {
                      "name": "transaction_hash",
                      "in": "query",
                      "description": "Transaction hash",
                      "required": true,
                      "type": "string"
                  }
              ],
              "responses": {
                  "200": {
                      "description": "Successful response.",
                      "schema": {
                          "type": "object",
                          "properties": {
                              "fee": {
                                  "type": "number",
                                  "description": "The fee in USDT"
                              }
                          }
                      }
                  },
                  "404": {
                      "description": "Transaction not found.",
                      "schema": {
                          "type": "object",
                          "properties": {
                              "error": {
                                  "type": "string",
                                  "description": "Error message"
                              }
                          }
                      }
                  }
              }
          }
      }
  }
}